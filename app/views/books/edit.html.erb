<!DOCTYPE html>
<html>
  <head>
    <title>WebCampBackendNew</title>
      <p><%= flash[:notice] %></p>
  </head>
  <body>
    <h1>Editing Book</h1>
    <%= form_with model: @book do |f| %>
      <h4>Title</h4>
        <%= f.text_field :title %>
      <h4>Body</h4>
        <%= f.text_area :body %>
        <%= f.submit 'Update' %>
    <% end %>
  </body>
</html>